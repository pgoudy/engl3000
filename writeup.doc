\pard\plain\ltrpar\ql\s30\itap0{\s30\f0\fs24\lang1033{\*\listtag0}\abinodiroverride\ltrch I am he.}{\s30\f0\fs24\lang1033{\*\listtag0}\par}
\pard\plain\ltrpar\ql\s30\itap0{\s30\f0\fs24\lang1033{\*\listtag0}\par}
